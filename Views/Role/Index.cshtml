@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>



<h1>List of Roles</h1>
<a asp-action="CreateRole">Create</a>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
</table>
<tbody>
    @foreach (var role in Model ?? Enumerable.Empty<Microsoft.AspNetCore.Identity.IdentityRole>())
    {
        <tr>
            <td>@(role.Id ?? "N/A")</td>
            <td>@(role.Name ?? "N/A")</td>
        </tr>
    }
</tbody>
</table>