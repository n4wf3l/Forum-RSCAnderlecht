@page
@model ResetPasswordConfirmationModel
@using RSCAnderlechtF.Models;
@inject LanguageService language
@{
    ViewData["Title"] = @language.Getkey("Reset password confirmation");
}

<h1>@ViewData["Title"]</h1>
<p>
    @language.Getkey("Your password has been reset.") <a asp-page="./Login">@language.Getkey("Click here to log in")</a>.
</p>
